                  }
                }
              }
            },
            ".social-wrapper": {
              "padding": "25px 0",
              "height": "100%",
              "@media (max-width: 992px)": {
                "text-align": "center"
              },
              ".social-wrap": {
                "height": "100%",
                "display": "inline-flex",
                "align-items": "center",
                "flex-wrap": "wrap",
                ".social-item": {
                  "display": "block",
                  "margin-right": "20px",
                  "cursor": "pointer",
                  "&:last-child": {
                    "margin-right": "0"
                  },
                  "&:hover, &:focus": {
                    ".mbr-iconfont": {
                      "color": "@socBg",
                      "background-color": "@socColor"
                    }
                  },
                  ".mbr-iconfont": {
                    "width": "30px",
                    "height": "30px",
                    "font-size": "16px",
                    "color": "@socColor",
                    "background-color": "@socBg",
                    "transition": "all 0.3s ease-in-out",
                    "border-radius": "50%",
                    "display": "flex",
                    "align-items": "center",
                    "justify-content": "center"
                  }
                }
              },
              "text-align": "right"
            },
            ".mbr-section-title": {
              "color": "#f2ac2b"
            },
            ".mbr-copy": {
              "color": "#d2d2df"
            }
