          "_customHTML": "<section data-bs-version=\"5.1\" class=\"footer01 authorm5\" group=\"Footer\" global once=\"footers\" mbr-class=\"{\n    'mbr-fullscreen': fullScreen,\n    'mbr-parallax-background': bg.parallax}\">\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\">\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"12\" step=\"1\" value=\"5\" condition=\"fullScreen == false\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"12\" step=\"1\" value=\"5\" condition=\"fullScreen == false\">\n        <input type=\"checkbox\" title=\"Reverse\" name=\"reverseContent\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Logo\" name=\"showLogo\" checked>\n        <input type=\"checkbox\" title=\"Navigation\" name=\"showNav\" checked>\n        <input type=\"color\" title=\"Active Color\" value=\"#f2ac2b\" name=\"active\" condition=\"showNav\">\n        <input type=\"checkbox\" title=\"Border\" name=\"showBorder\" checked>\n        <input type=\"color\" title=\"Border Color\" value=\"#284177\" name=\"border\" condition=\"showBorder\">\n        <input type=\"checkbox\" title=\"Social\" name=\"showSocial\" checked>\n        <input type=\"range\" inline title=\"Social Amount\" name=\"soc\" min=\"0\" max=\"5\" step=\"1\" value=\"4\" condition=\"showSocial\">\n        <input type=\"color\" title=\"Social Color\" value=\"#232b45\" name=\"socColor\" condition=\"showSocial\" selected>\n        <input type=\"color\" title=\"Social Bg\" value=\"#f2ac2b\" name=\"socBg\" condition=\"showSocial\" selected>\n        <input type=\"checkbox\" title=\"Copyright\" name=\"showCopy\" checked>\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Image\" value=\"@PROJECT_PATH@/assets/images/background1.jpg\" parallax selected>\n            <input type=\"color\" title=\"Color\" value=\"#232b45\">\n        </fieldset>\n        <header condition=\"bg.type === 'video'\">Fallback Image</header>\n        <input type=\"image\" title=\"Fallback Image\" value=\"@PROJECT_PATH@/assets/images/background1.jpg\" name=\"fallBackImage\" condition=\"bg.type === 'video'\">\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#232b45\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\" mbr-class=\"{'flex-row-reverse' : reverseContent}\">\n            <div class=\"col-12 col-lg-8\" mbr-if=\"showTitle || showLogo\">\n                <div class=\"title-wrapper\">\n                    <div class=\"title-wrap\">\n                        <img src=\"@PROJECT_PATH@/assets/images/rostro-108x106.png\" alt=\"Fundación MISOMBA\" mbr-if=\"showLogo\">\n                        <h2 class=\"mbr-section-title mbr-fonts-style\" data-app-selector=\".mbr-section-title, .title-wrapper\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle\">\n                            FUNDACIÓN MISOMBA</h2>\n                    </div>\n                </div>\n            </div>\n            <div class=\"col-12 col-lg-4\" mbr-if=\"showNav\">\n                <ul class=\"list mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".list, .item-wrap\" data-multiline mbr-article>\n                    <li class=\"item-wrap\">PROYECTOS</li>\n                    <li class=\"item-wrap\">DONACIONES</li>\n                    <li class=\"item-wrap\">CONTACTANOS</li>\n                </ul>\n            </div>\n            <div class=\"col-12\" mbr-if=\"showBorder\">\n                <div class=\"border-wrap\"></div>\n            </div>\n            <div class=\"col-12 col-lg-6\" mbr-if=\"showCopy\">\n                <div class=\"copy-wrapper\">\n                    <p class=\"mbr-copy mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-copy, .social-wrapper\"><b>FUNDACION MISOMBA</b> - Con el apoyo tecnológico de <b><a href=\"https://digital01.com\" class=\"text-primary\" target=\"_blank\">01 SAS</a></b></p>\n                </div>\n            </div>\n            <div class=\"col-12 col-lg-6\" mbr-if=\"showSocial\">\n                <div class=\"social-wrapper\">\n                    <div class=\"social-wrap\">\n                        <a href=\"https://www.instagram.com/fundacionmisomba/\" class=\"social-item\" target=\"_blank\">\n                            <span mbr-icon class=\"mbr-iconfont socicon-instagram socicon\"></span>\n                        </a>\n                        <a href=\"https://web.facebook.com/FundacionMisomba/?locale=es_LA&_rdc=1&_rdr\" class=\"social-item\" mbr-if=\"soc > 1\" target=\"_blank\">\n                            <span mbr-icon class=\"mbr-iconfont socicon-facebook socicon\"></span>\n                        </a>\n                        <a href=\"https://co.linkedin.com/in/fundacion-misomba-247a8b272\" class=\"social-item\" mbr-if=\"soc > 2\" target=\"_blank\">\n                            <span mbr-icon class=\"mbr-iconfont socicon-linkedin socicon\"></span>\n                        </a>\n                        <a href=\"https